{
  "expo": {
    "name": "SWDiceRoller",
    "slug": "SWDiceRoller",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logos/YellowDiceAppIcon1.png",
    "scheme": "SWDiceRoller",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logos/YellowDiceAppIcon1.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.trent.SWDiceRoller"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logos/YellowDiceAppIcon1.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e4b7b2a4-3f75-4e0f-9709-75bec738d3ef"
      }
    }
  }
}
